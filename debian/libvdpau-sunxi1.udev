# Do not edit this file, it will be overwritten on update
#
# Set the right permissions to the devices provided by the
# armsoc driver
KERNEL=="cedar_dev", MODE="0770", GROUP="video"
KERNEL=="ion", MODE="0770", GROUP="video"

ENV{ACL_MANAGE}=="0", GOTO="vpu_end"
ACTION!="add|change", GOTO="vpu_end"

# graphics devices
ACTION=="add|change", KERNEL=="mali*", TAG+="udev-acl"

LABEL="vpu_end"

